n = int(input())
NumList = []
for i in range(n):
    num=int(input())
    NumList.append(num)
for i in range(n):
    if NumList[i] < 0:
        print(NumList[i])
for i in range(n):
    if NumList[i] == 0:
        print(NumList[i])
for i in range(n):
    if NumList[i] > 0:
        print(NumList[i])

#Можно сделать 3 разныз списка и потом выводить их по очереди