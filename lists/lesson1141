n = int(input())
numlist = []
numlistnew = []
for i in range(n):
    num=int(input())
    numlist.append(num)
#for i in range(len(numlist)):
#   print(numlist[i])

print(*numlist, sep='\n')

print()
for i in range(len(numlist)):
    x = numlist[i]**2+2*numlist[i]+1
    numlistnew.append(x)
    print(numlistnew[i])